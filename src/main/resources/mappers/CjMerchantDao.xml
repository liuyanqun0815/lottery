<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cj.lottery.dao.CjMerchantDao">

  <sql id="Base_Column_List">
    id, merchant_code, appid, mch_id, `status`
  </sql>

  <update id="updateByPrimaryKeySelective" parameterType="com.cj.lottery.domain.CjMerchant">
    update cj_merchant
    <set>
      <if test="merchantCode != null">
        merchant_code = #{merchantCode},
      </if>
      <if test="appid != null">
        appid = #{appid},
      </if>
      <if test="mchId != null">
        mch_id = #{mchId},
      </if>
      <if test="status != null">
        `status` = #{status},
      </if>

    </set>
    where id = #{id}
  </update>
  <select id="selelctAllMerchant" resultType="com.cj.lottery.domain.CjMerchant">
    select <include refid="Base_Column_List"/>
    from cj_merchant
    where status = 0
    and is_delete = 0
  </select>

</mapper>